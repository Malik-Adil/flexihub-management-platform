{
  "name": "@flexihub-management-platform/gateway-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@flexihub-management-platform/gateway"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/gateway-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@flexihub-management-platform/gateway:build",
          "@flexihub-management-platform/gateway:serve"
        ]
      }
    }
  }
}
